{
    "name": "rajibbinalam/country-state-city",
    "description": "find country state and cities all over the world",
    "keywords": [
        "country-state-city",
        "laravel",
        "country",
        "state",
        "city"
    ],
    "homepage": "https://github.com/rajibbinalam/country-state-city",
    "license": "MIT",
    "authors": [
        {
            "name": "rajibbinalam",
            "email": "rajibbinalam@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Rajib\\LaravelCountryStateCity\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false,
            "phpstan/extension-installer": false
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rajib\\LaravelCountryStateCity\\CountryStateCityServiceProvider"
            ],
            "aliases": {
                "CountryStateCity": "Rajib\\LaravelCountryStateCity\\CountryStateCity"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
